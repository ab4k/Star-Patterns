#include <stdio.h>
void main() {
    for (int i = 1; i <= 4; i++) {
        char k = 'A';
        for(int j = 1; j <= 7; j++) {
            if(j <= 5-i || j >= 3+i){
                printf("%c", k);
                if(j < 4)
                    k++;
                else
                    k--;
            }
            else{
                printf(" ");
                if (j == 4)
                    k--;
            }
        }
        printf("\n");
    }
}
