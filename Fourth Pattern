#include <stdio.h>

int main() {
    
    int m, n;
    printf("Enter the value of m: ");
    scanf("%d", &m);
    
    printf("Enter the value of n: ");
    scanf("%d", &n);
    
    for (int i = 1; i <= m; i++) {
        for (int j = 1; j <= n; j++) {
            if (j <= m+1-i)
                printf("*");
            else 
                printf(" ");
        }
        printf("\n");
    }
    
    return 0;
}
